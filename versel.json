{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/static/:path*",
            "destination": "/static/:path*"
        },
        {
            "source": "/favicon.ico",
            "destination": "/favicon.ico"
        },
        {
            "source": "/manifest.json",
            "destination": "/manifest.json"
        },
        {
            "source": "/:path*",
            "destination": "/index.html"
        }
    ]
}